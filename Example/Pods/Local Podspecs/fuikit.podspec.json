{
  "name": "fuikit",
  "version": "0.1.5",
  "summary": "fuikit makes standard iOS classes easy to use with FP patterns.",
  "swift_versions": [
    "4.0",
    "4.1",
    "4.2",
    "5.0",
    "5.1",
    "5.2"
  ],
  "description": "fuikit makes standard iOS classes easy to use with Functional Programming by adding var function properties.\n\nFor instance, instead of subclassing UIViewController, you could use FUIViewController and set the onViewDidLoad property to whatever you'd like.",
  "homepage": "https://github.com/ThryvInc/fuikit",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Elliot": ""
  },
  "source": {
    "git": "https://github.com/ThryvInc/fuikit.git",
    "tag": "0.1.5",
    "submodules": true
  },
  "social_media_url": "https://twitter.com/elliot_schrock",
  "platforms": {
    "ios": "11.0"
  },
  "source_files": "fuikit/Classes/**/*.swift",
  "resources": "fuikit/**/*.xib",
  "subspecs": [
    {
      "name": "Core",
      "source_files": "fuikit/Classes/Core/**/*.swift",
      "resources": "fuikit/Classes/Core/**/*.xib"
    },
    {
      "name": "CoreLocation",
      "source_files": "fuikit/Classes/FCL/*.swift"
    },
    {
      "name": "CoreBluetooth",
      "source_files": "fuikit/Classes/FCB/*.swift"
    }
  ],
  "swift_version": "5.2"
}
